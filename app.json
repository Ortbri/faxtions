{
  "expo": {
    "name": "faxtions",
    "slug": "faxtions",
    "version": "1.0.0",
    "orientation": "portrait",
    "owner": "colony-app",
    "icon": "./src/assets/images/faxtionApp.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/faxtionApp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ortbit02.faxtions"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/faxtionApp.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ortbit02.faxtions"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./src/assets/images/faxtionApp.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/faxtionApp.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      ["expo-font"]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactServerFunctions": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f138485b-155a-4583-9cea-ca581d4361f1"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f138485b-155a-4583-9cea-ca581d4361f1"
    }
  }
}
