{
  "expo": {
    "name": "faxtions",
    "slug": "faxtions",
    "version": "1.0.0",
    "orientation": "portrait",
    "owner": "colony-app",
    "icon": "./src/assets/images/faxtionsApp.png",
    "scheme": "faxtions",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/faxtionsLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ortbit02.faxtions"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/faxtionsApp.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.ortbit02.faxtions"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./src/assets/images/faxtionsLogo.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "http://localhost:8081"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/faxtionsLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a"
        }
      ],
      [
        "expo-font"
      ]
    ],
    "experiments": {
      "reactServerFunctions": true,
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "http://localhost:8081"
      },
      "eas": {
        "projectId": "d8640f3c-e8c7-4496-abf7-b0cc7f3002ea"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f138485b-155a-4583-9cea-ca581d4361f1"
    }
  }
}
